size_t _strspn(const char *s, const char *accept)
{
    size_t len = 0;
    const char *p;

    while (*s)
    {
        for (p = accept; *p; p++)
        {
            if (*s == *p)
                break;
        }
        if (*p == '\0')
            return len;
        len++;
        s++;
    }

    return len;
}

